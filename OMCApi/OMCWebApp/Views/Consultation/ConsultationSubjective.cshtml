@model  OMCWebApp.Models.ConsultationSubjectiveModel

@using (Html.BeginForm("InsertUpdateConsultationSubjectives", "Consultation", FormMethod.Post))
{
    <h2>Manage Consultation Subjective</h2>
    @Html.AntiForgeryToken()
    <table class="createConsAllergy">
        <tr>
            <td>
                Onset:
            </td>
            <td>
                @Html.HiddenFor(m => m.ConsultationSubjectivesObject.Id, Model.ConsultationSubjectivesObject.Id)
                @Html.HiddenFor(m => m.ConsultationSubjectivesObject.AddedBy, Model.ConsultationSubjectivesObject.AddedBy)
                @Html.HiddenFor(m => m.ConsultationSubjectivesObject.ModifiedBy, Model.ConsultationSubjectivesObject.ModifiedBy)
                @Html.HiddenFor(m => m.ConsultationSubjectivesObject.ConsultationId, Model.ConsultationSubjectivesObject.ConsultationId)
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.Onset
                , Model.ConsultationSubjectivesObject.Onset)            
            </td>
        </tr>    
        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Duration:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.Duration
