            </td>
        </tr>  
        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Location:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.Location
                , Model.ConsultationSubjectivesObject.Location)
            </td>
        </tr>  
        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Character:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.Character
                , Model.ConsultationSubjectivesObject.Character)
            </td>
        </tr> 
        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Alleviating Factors:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.AlleviatingFactors
                , Model.ConsultationSubjectivesObject.AlleviatingFactors)
            </td>
        </tr> 
        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Aggravating Factors:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.AggravatingFactors
                , Model.ConsultationSubjectivesObject.AggravatingFactors)
            </td>
        </tr> 
        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Radiation:
