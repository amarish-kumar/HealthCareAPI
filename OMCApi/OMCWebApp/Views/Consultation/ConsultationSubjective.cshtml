        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Temporal Pattern:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.TemporalPattern
                , Model.ConsultationSubjectivesObject.TemporalPattern)
            </td>
        </tr> 
        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Severity:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.Severity
                , Model.ConsultationSubjectivesObject.Severity)
            </td>
        </tr> 
        <tr style="height:20px;"></tr>
        <tr>
            <td>
                Chronology:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.Chronology
                , Model.ConsultationSubjectivesObject.Chronology)
            </td>
        </tr> 
        <tr style="height:20px;"></tr> 
        <tr>
            <td>
                Additional Symptoms:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.AdditionalSymptoms
                , Model.ConsultationSubjectivesObject.AdditionalSymptoms)
            </td>
        </tr>
        <tr style="height:20px;"></tr>   
        <tr>
            <td>
                Allergies:
            </td>
            <td>
                @Html.TextBoxFor(m => m.ConsultationSubjectivesObject.Allergies
                , Model.ConsultationSubjectivesObject.Allergies)
            </td>
        </tr>
        <tr style="height:20px;"></tr>   
        <tr>
            <td colspan="2">
                <button type="submit">
                    @(Model.ConsultationSubjectivesObject.Id == 0 ? "Create Consultation Subjective" : "Update Consultation Subjective")
                </button>
            </td>
        </tr>
    </table>
}
